{
  "name": "vue-datetime3-demo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "demo": "concurrently \"npm run demo --workspace=demo\" \"npm run watch --workspace=vue-datetime3\"",
    "build": "npm run build --workspaces --if-present",
    "tsc": "vue-tsc --noEmit -p tsconfig.tsc.json"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "concurrently": "^8.0.1",
    "pre-commit": "^1.2.0",
    "typescript": "^4.9.5"
  },
  "pre-commit": "--no-verify",
  "engines": {
    "node": "^18.13.0",
    "npm": "^8.19.0"
  }
}
